{
  "manifest_version": 2,

  "name": "Mars Forecast",
  "description": "Shows weather information for a specific date on Mars",
  "version": "1.0",



  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Mars Weather"
  },



  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}
