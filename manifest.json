{
  "manifest_version": 2,

  "name": "Mars Weather",
  "description": "Shows weather information from the Curiosity rover on Mars",
  "version": "0.2",



  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Mars Weather"
  },



  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}
